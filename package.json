{
  "name": "vampa",
  "version": "0.0.0",
  "description": "My Cloudflare Pages Functions plugins & Hono middlewares",
  "license": "MIT",
  "private": true,
  "packageManager": "npm@9.6.6",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "turbo run format",
    "lint": "turbo run lint",
    "ncu": "ncu --dep 'dev,prod' --interactive --deep",
    "publish": "npm run build && npx changeset publish",
    "size": "turbo run size --filter=./packages/* --concurrency=1",
    "test": "turbo run test --concurrency=1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@cloudflare/workers-types": "^4.20231218.0",
    "@turbo/gen": "^1.11.3",
    "eslint": "^8.56.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "npm-check-updates": "^16.14.12",
    "pkg-size": "^2.4.0",
    "prettier": "^3.1.1",
    "prettier-config-custom": "*",
    "rimraf": "^5.0.5",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.4"
  }
}
